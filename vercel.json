{
    "version": 2,
    "builds": [
        {
            "src": "dist/api/index.mjs",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/public"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/dist/api/index.mjs"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/favicon.png",
            "dest": "/favicon.png"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}